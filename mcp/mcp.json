{
  "financial": {
    "transport": "stdio",
    "command": "uv",
    "args": [
      "--directory",
      "/Users/dmc/domgoer/investment-advisor/mcp/vendors/financial-datasets-mcp",
      "run",
      "server.py"
    ],
    "env": {
      "FINANCIAL_DATASETS_API_KEY": "${FINANCIAL_DATASETS_API_KEY}"
    }
  },
  "google-search": {
    "transport": "stdio",
    "command": "node",
    "args": [
      "/Users/dmc/domgoer/investment-advisor/mcp/vendors/Google-Search-MCP-Server/dist/google-search.js"
    ],
    "env": {
      "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
      "GOOGLE_SEARCH_ENGINE_ID": "${GOOGLE_SEARCH_ENGINE_ID}"
    }
  }
}
